            "TIMEOUT_MS": 60000,
            "RETRY_INTERVAL_MS": 2000
        }
    },
    "scenarios": [
        {
            "id": "SCN_MANUAL_08_JAN",
            "description": "Verify Backfill 2026-01-08 completes and generates footprint.",
            "steps": [
                {
                    "step_id": "1_poll_status",
                    "action": "POLL",
                    "url": "{AGG_BASE}/debug-r2?key=seed_status/2026-01-08.json",
                    "interval_ms": 2000,
                    "max_attempts": 60,
                    "break_condition": "json.status == 'COMPLETED' || json.status == 'FAILED' || (json.processed && json.processed > 5000)"
                },
                {
                    "step_id": "2_verify_footprint_engine_1",
                    "description": "Verify Engine 1 has data (likely for hour 07 or 14)",
                    "action": "GET",
                    "url": "{ENGINE_BASE}/debug/hour?ticker=BBCA&y=2026&m=01&d=08&h=07",
                    "manual_check": true,
                    "expected_logic": "Check if any lines returned"
                },
                {
                    "step_id": "3_verify_footprint_engine_2",
                    "description": "Verify Engine 2 has data (likely for hour 01 or 08)",
                    "action": "GET",
                    "url": "{ENGINE_BASE}/debug/hour?ticker=BBCA&y=2026&m=01&d=08&h=01",
                    "expect": {
                        "status": 200
                    }
                }
            ]
        }
    ]
